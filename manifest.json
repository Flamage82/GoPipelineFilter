{
    "manifest_version": 2,
    "name": "Go Pipeline Filter",
    "description": "Filters and collapses pipelines in Go Continuous Delivery.",
    "version": "1.0",
    "converted_from_user_script": true,
    "permissions": [ "storage" ],
    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [  ],
        "js":               [ "script.js" ],
        "matches":          [ "*://*/go/pipelines*", "*://*/go/home*" ],
        "run_at": "document_end"
    } ],
    "icons": { 
        "16": "logo_16.png",
        "48": "logo_48.png",
        "128": "logo_128.png"
    },
    "web_accessible_resources": [ "script_embed.js" ]
}